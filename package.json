{
    "name": "clang-tidy-linter",
    "displayName": "clang-tidy-linter",
    "description": "",
    "version": "0.0.1",
    "publisher": "alesiong",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension",
    "activationEvents": [
        "onLanguage:cpp",
        "onLanguage:c"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/js-yaml": "^3.11.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "js-yaml": "^3.12.0"
    }
}